<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>

	<!-- Inherit the widgets library       .                  -->
        <inherits name='org.gwtwidgets.WidgetLibrary'/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='org.openhc.trowser.gwt.client.Main'/>

	<!-- The RPC point                                        -->
	<servlet path="/Service"
                 class='org.openhc.trowser.gwt.server.ServiceImpl'/>

</module>
